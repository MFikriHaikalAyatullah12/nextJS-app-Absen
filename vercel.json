{
  "buildCommand": "cd absensi-app && npm run build",
  "outputDirectory": "absensi-app/.next",
  "installCommand": "cd absensi-app && npm install",
  "devCommand": "cd absensi-app && npm run dev",
  "framework": "nextjs",
  "env": {
    "DATABASE_URL": "@database_url",
    "NEXTAUTH_SECRET": "@nextauth_secret",
    "JWT_SECRET": "@jwt_secret"
  }
}
